        <Grid.RowDefinitions>
            <RowDefinition Height="1*" />
            <RowDefinition Height="11*" />
        </Grid.RowDefinitions>
        <WrapPanel VerticalAlignment="Top" HorizontalAlignment="Right">
            <Button Command="{Binding Exit_btn}" Margin="0,10,10,0" Height="40" Width="40" HorizontalAlignment="Left" VerticalAlignment="Center" >X</Button>
        </WrapPanel>
            
           
            
        <WrapPanel Grid.Row="1" Name="Team_List" >
           
        </WrapPanel>
        </Grid>
</Window>
