        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        mc:Ignorable="d" Name="Windows_Screen"
        Title="MainWindow"  Height="550" BorderThickness="10" Width="1000" MinWidth="700" MinHeight="450" MouseLeftButtonDown="OnMouseLeftButtonDown" WindowStyle="None" AllowsTransparency="True" Background="#CC0F1728" BorderBrush="#8C673AB7" >
